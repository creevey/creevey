{
  "name": "creevey",
  "description": "creevey is a tool for automated visual testing, that tightly integrated with storybook",
  "version": "0.7.0-beta.1",
  "bin": {
    "creevey": "./lib/cli.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Dmitriy Lazarev <w@kich.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKich/creevey.git"
  },
  "license": "MIT",
  "scripts": {
    "commit": "git-cz",
    "clean": "rimraf lib",
    "lint": "run-p --continue-on-error lint:*",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --ext .js,.ts,.tsx ./",
    "lint:prettier": "prettier --check *.{md,json,js,ts} ./**/*.{js,ts,tsx}",
    "test": "run-p test:*",
    "test:unit": "mocha",
    "start": "cross-env NODE_ENV=test run-p start:*",
    "start:client": "webpack-dev-server",
    "start:storybook": "start-storybook --ci -p 6006",
    "start:creevey": "wait-on -t 30000 http-get://localhost:6006/ && yarn creevey --ui",
    "creevey": "node --require ./scripts/babel-register -- src/cli",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint --fix ./",
    "fix:prettier": "prettier --write *.{md,json,js,ts} ./**/*.{js,ts,tsx}",
    "prebuild": "yarn clean && mkdir lib",
    "build": "run-p --continue-on-error build:*",
    "build:types": "tsc --emitDeclarationOnly -p scripts/tsconfig.prod.json",
    "build:creevey": "babel src --out-dir lib --extensions .ts,.tsx --ignore src/client",
    "build:client": "webpack --mode production",
    "postbuild": "cp scripts/dist/* lib/",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "prepublishOnly": "yarn build"
  },
  "engines": {
    "node": ">=10.10"
  },
  "peerDependencies": {
    "@storybook/core": "4.* || 5.* || 6.*"
  },
  "dependencies": {
    "@babel/generator": "^7.10.5",
    "@babel/parser": "^7.10.5",
    "@babel/traverse": "^7.10.5",
    "@babel/types": "^7.10.5",
    "@storybook/csf": "^0.0.1",
    "@types/chai": "^4.2.12",
    "@types/selenium-webdriver": "^4.0.9",
    "airbnb-js-shims": "^2.2.1",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.1",
    "core-js": "^3.6.5",
    "event-hooks-webpack-plugin": "^2.1.7",
    "event-source-polyfill": "^1.0.15",
    "jsdom": "^16.3.0",
    "jsdom-global": "^3.0.2",
    "koa": "^2.13.0",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.19",
    "minimist": "^1.2.5",
    "mocha": "~8.0.1",
    "pixelmatch": "^5.2.1",
    "pngjs": "^5.0.0",
    "regenerator-runtime": "^0.13.7",
    "schema-utils": "^2.7.0",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "tsconfig-paths": "^3.9.0",
    "webpack-node-externals": "^2.5.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/register": "^7.10.5",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.28",
    "@skbkontur/react-icons": "^4.3.1",
    "@skbkontur/react-ui": "2.4.0",
    "@storybook/addons": "^6.0.21",
    "@storybook/api": "^6.0.21",
    "@storybook/channels": "^6.0.21",
    "@storybook/client-api": "^6.0.21",
    "@storybook/client-logger": "^6.0.21",
    "@storybook/core-events": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@types/babel__generator": "^7.6.1",
    "@types/babel__traverse": "^7.0.13",
    "@types/event-hooks-webpack-plugin": "^2.0.0",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/koa": "^2.11.3",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-static": "^4.0.1",
    "@types/loader-utils": "^2.0.1",
    "@types/lodash": "^4.14.158",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.26",
    "@types/pixelmatch": "^5.2.1",
    "@types/pngjs": "^3.4.2",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.21",
    "@types/webpack-dev-server": "^3.11.0",
    "@types/webpack-node-externals": "^2.5.0",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "babel-loader": "^8.1.0",
    "conventional-changelog-cli": "^2.0.34",
    "cross-env": "^7.0.2",
    "css-loader": "^4.0.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "git-cz": "^4.7.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "immer": "^7.0.7",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "typescript": "^3.9.7",
    "use-immer": "^0.4.1",
    "wait-on": "^5.1.0",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.(j|t)s?(x)": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.(md|json)": [
      "prettier --write"
    ]
  }
}
