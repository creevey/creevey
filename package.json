{
  "name": "creevey",
  "description": "Cross-browser screenshot testing tool for Storybook with fancy UI Runner",
  "keywords": [
    "testing",
    "storybook",
    "storybook-addons",
    "visual-testing",
    "visual-regression",
    "screenshot-tests",
    "screenshots",
    "storybook-addon",
    "addon",
    "test"
  ],
  "version": "0.8.0-beta.0",
  "bin": {
    "creevey": "./lib/cjs/cli.js"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "author": "Dmitriy Lazarev <w@kich.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKich/creevey.git"
  },
  "license": "MIT",
  "scripts": {
    "commit": "git-cz",
    "clean": "rimraf lib",
    "lint": "run-p --continue-on-error lint:*",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --ext .js,.ts,.tsx ./",
    "lint:prettier": "prettier --check *.{md,json,js,ts} ./**/*.{js,ts,tsx}",
    "pretest:e2e": "yarn build && yarn pack -f creevey.tgz",
    "bootstrap:storybook": "cd ../storybook && yarn install && yarn bootstrap --core",
    "test": "run-p test:*",
    "test:unit": "mocha ./tests/{client,server}/**/*.test.ts ./tests/*.test.ts",
    "test:e2e": "mocha ./tests/e2e/*.test.ts",
    "start": "run-p start:*",
    "start:client": "webpack-dev-server",
    "start:storybook": "start-storybook --ci -p 6006",
    "start:creevey": "yarn creevey --ui",
    "start:creevey:github": "yarn creevey:github --ui",
    "creevey": "node --require ./scripts/babel-register -- src/cli",
    "creevey:github": "yarn creevey --config .creevey/github.config.ts",
    "creevey:gitlab": "yarn creevey --config .creevey/gitlab.config.ts",
    "creevey:hybrid": "yarn creevey --config .creevey/hybrid.config.ts",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint --fix ./",
    "fix:prettier": "prettier --write .",
    "prebuild": "yarn clean && mkdir lib",
    "build": "run-p --continue-on-error build:*",
    "build:types": "tsc --emitDeclarationOnly -p scripts/tsconfig.prod.json",
    "build:creevey": "run-p \"build:creevey:* --extensions .ts,.tsx --ignore src/client/web\"",
    "build:creevey:cjs": "cross-env BABEL_MODE=cjs babel src --out-dir lib/cjs",
    "build:creevey:esm": "cross-env BABEL_MODE=esm babel src --out-dir lib/esm",
    "build:client": "webpack --mode production",
    "postbuild": "run-p copy:*",
    "copy:index": "cpx scripts/dist/types/* lib/types/",
    "copy:html": "cpx src/client/web/index.html lib/cjs/client/web",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --project-token t3010eknrh",
    "prepublishOnly": "yarn build",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=10.10"
  },
  "dependencies": {
    "@babel/code-frame": "^7.12.13",
    "@babel/generator": "^7.15.0",
    "@babel/helpers": "^7.15.3",
    "@babel/parser": "^7.15.3",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@babel/runtime-corejs3": "^7.15.3",
    "@babel/traverse": "^7.15.0",
    "@babel/types": "^7.15.0",
    "@koa/cors": "^3.2.0",
    "@octokit/core": "^3.3.1",
    "@storybook/components": "^6.4.19",
    "@storybook/csf": "0.0.2--canary.87bc651.0",
    "@storybook/testing-library": "^0.0.9",
    "@storybook/theming": "^6.4.19",
    "@types/chai": "^4.2.15",
    "@types/selenium-webdriver": "^4.0.12",
    "airbnb-js-shims": "^2.2.1",
    "chai": "^4.3.4",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.1",
    "core-js": "^3.16.3",
    "dockerode": "^3.2.1",
    "event-source-polyfill": "1.0.25",
    "find-cache-dir": "^3.3.1",
    "global-jsdom": "8.3.0",
    "jsdom": "^16.5.2",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.7.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "micromatch": "^4.0.4",
    "minimist": "^1.2.5",
    "mocha": "~9.1.0",
    "null-loader": "^4.0.1",
    "ora": "^5.4.0",
    "pirates": "^4.0.1",
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "polished": "^4.1.1",
    "qs": "^6.10.1",
    "regenerator-runtime": "^0.13.7",
    "resolve-from": "^5.0.0",
    "schema-utils": "^3.0.0",
    "selenium-webdriver": "^4.0.0-beta.3",
    "shelljs": "^0.8.4",
    "tsconfig-paths": "^3.11.0",
    "webpack-node-externals": "^3.0.0",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.15.0",
    "@mdx-js/mdx": "^1.6.22",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.4.19",
    "@storybook/api": "^6.4.19",
    "@storybook/channels": "^6.4.19",
    "@storybook/client-api": "^6.4.19",
    "@storybook/client-logger": "^6.4.19",
    "@storybook/core": "^6.4.19",
    "@storybook/core-client": "^6.4.19",
    "@storybook/core-common": "^6.4.19",
    "@storybook/core-events": "^6.4.19",
    "@storybook/core-server": "^6.4.19",
    "@storybook/preview-web": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@types/babel__code-frame": "^7.0.2",
    "@types/babel__generator": "^7.6.2",
    "@types/babel__traverse": "^7.11.1",
    "@types/dockerode": "^3.2.3",
    "@types/find-cache-dir": "^3.2.0",
    "@types/koa": "^2.13.1",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-mount": "^4.0.1",
    "@types/koa-static": "^4.0.1",
    "@types/koa__cors": "^3.1.1",
    "@types/loader-utils": "^2.0.2",
    "@types/lodash": "^4.14.172",
    "@types/micromatch": "^4.0.1",
    "@types/minimist": "^1.2.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.14.37",
    "@types/pixelmatch": "^5.2.2",
    "@types/pngjs": "^6.0.0",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.3",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/shelljs": "^0.8.8",
    "@types/tmp": "^0.2.0",
    "@types/webpack": "^4.41.26",
    "@types/webpack-dev-server": "^3.11.3",
    "@types/webpack-node-externals": "^2.5.1",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-loader": "^8.2.1",
    "browserstack-local": "^1.4.8",
    "chromatic": "^5.7.0",
    "conventional-changelog-cli": "^2.1.1",
    "cpx": "1.5.0",
    "cross-env": "7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "git-cz": "^4.7.4",
    "husky": "^7.0.2",
    "immer": "^9.0.1",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "pinst": "^2.1.6",
    "prettier": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rimraf": "^3.0.2",
    "tmp": "^0.2.1",
    "typescript": "~4.4.2",
    "use-immer": "^0.6.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  },
  "lint-staged": {
    "**/*.(j|t)s?(x)": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.(md|json)": [
      "prettier --write"
    ]
  },
  "storybook": {
    "displayName": "Creevey"
  },
  "volta": {
    "node": "14.19.0",
    "yarn": "1.22.17"
  }
}
