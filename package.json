{
  "name": "creevey",
  "description": "Cross-browser screenshot testing tool for Storybook with fancy UI Runner.",
  "keywords": [
    "testing",
    "storybook",
    "storybook-addons",
    "visual-testing",
    "visual-regression",
    "screenshot-tests",
    "screenshots"
  ],
  "version": "0.7.24",
  "bin": {
    "creevey": "./lib/cli.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Dmitriy Lazarev <w@kich.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKich/creevey.git"
  },
  "license": "MIT",
  "scripts": {
    "commit": "git-cz",
    "clean": "rimraf lib",
    "lint": "run-p --continue-on-error lint:*",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --ext .js,.ts,.tsx ./",
    "lint:prettier": "prettier --check *.{md,json,js,ts} ./**/*.{js,ts,tsx}",
    "test": "run-p test:*",
    "test:unit": "mocha ./tests/{client,server}/**/*.test.ts ./tests/*.test.ts",
    "test:e2e": "mocha ./tests/e2e/**/*.test.ts",
    "start": "cross-env NODE_ENV=test run-p start:*",
    "start:client": "webpack-dev-server",
    "start:storybook": "start-storybook --ci -p 6006",
    "start:creevey": "wait-on -t 60000 http-get://localhost:6006/ && yarn creevey --ui",
    "creevey": "node --require ./scripts/babel-register -- src/cli",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint --fix ./",
    "fix:prettier": "prettier --write .",
    "prebuild": "yarn clean && mkdir lib",
    "build": "run-p --continue-on-error build:*",
    "build:types": "tsc --emitDeclarationOnly -p scripts/tsconfig.prod.json",
    "build:creevey": "babel src --out-dir lib --extensions .ts,.tsx --ignore src/client/web",
    "build:client": "webpack --mode production",
    "postbuild": "cp scripts/dist/* lib/ && cp src/client/web/index.html lib/client/web",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --project-token t3010eknrh",
    "prepublishOnly": "yarn build && pinst --disable",
    "postinstall": "husky install",
    "postpublish": "pinst --enable"
  },
  "engines": {
    "node": ">=10.10"
  },
  "dependencies": {
    "@babel/code-frame": "^7.12.13",
    "@babel/generator": "^7.13.9",
    "@babel/parser": "^7.13.9",
    "@babel/traverse": "^7.13.0",
    "@babel/types": "^7.13.0",
    "@octokit/core": "^3.2.5",
    "@storybook/components": "^6.1.21",
    "@storybook/csf": "^0.0.1",
    "@storybook/theming": "^6.1.21",
    "@types/chai": "^4.2.15",
    "@types/selenium-webdriver": "^4.0.11",
    "airbnb-js-shims": "^2.2.1",
    "chai": "^4.3.3",
    "chalk": "^4.1.0",
    "chokidar": "^3.5.1",
    "core-js": "^3.9.1",
    "dockerode": "^3.2.1",
    "event-source-polyfill": "^1.0.21",
    "find-cache-dir": "^3.3.1",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "koa": "^2.13.1",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "loader-utils": "^2.0.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "mocha": "~8.3.0",
    "null-loader": "^4.0.1",
    "ora": "^5.3.0",
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "polished": "^3.7.1",
    "qs": "^6.9.6",
    "regenerator-runtime": "^0.13.7",
    "schema-utils": "^3.0.0",
    "selenium-webdriver": "^4.0.0-beta.1",
    "shelljs": "^0.8.4",
    "tsconfig-paths": "^3.9.0",
    "webpack-node-externals": "^2.5.2",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.8",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addons": "^6.1.21",
    "@storybook/api": "^6.1.21",
    "@storybook/channels": "^6.1.21",
    "@storybook/client-api": "^6.1.21",
    "@storybook/client-logger": "^6.1.21",
    "@storybook/core": "^6.1.21",
    "@storybook/core-events": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@types/babel__code-frame": "^7.0.2",
    "@types/babel__generator": "^7.6.2",
    "@types/babel__traverse": "^7.0.15",
    "@types/dockerode": "^3.2.2",
    "@types/find-cache-dir": "^3.2.0",
    "@types/koa": "^2.13.1",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-static": "^4.0.1",
    "@types/loader-utils": "^2.0.1",
    "@types/lodash": "^4.14.168",
    "@types/minimist": "^1.2.1",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@types/pixelmatch": "^5.2.2",
    "@types/pngjs": "^6.0.0",
    "@types/qs": "^6.9.5",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/shelljs": "^0.8.8",
    "@types/tmp": "^0.2.0",
    "@types/webpack": "^4.41.26",
    "@types/webpack-dev-server": "^3.11.1",
    "@types/webpack-node-externals": "^2.5.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-loader": "^8.2.1",
    "chromatic": "^5.6.3",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "git-cz": "^4.7.4",
    "husky": "^5.1.3",
    "immer": "^8.0.0",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "pinst": "^2.1.6",
    "prettier": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "rimraf": "^3.0.2",
    "tmp": "^0.2.1",
    "typescript": "^4.2.3",
    "use-immer": "^0.4.2",
    "wait-on": "^5.2.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  },
  "lint-staged": {
    "**/*.(j|t)s?(x)": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.(md|json)": [
      "prettier --write"
    ]
  }
}
