{
  "name": "creevey",
  "description": "Cross-browser screenshot testing tool for Storybook with fancy UI Runner",
  "keywords": [
    "testing",
    "storybook",
    "storybook-addons",
    "visual-testing",
    "visual-regression",
    "screenshot-tests",
    "screenshots",
    "storybook-addon",
    "addon",
    "test"
  ],
  "version": "0.9.0-beta.4",
  "bin": {
    "creevey": "./lib/cjs/cli.js"
  },
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "default": "./lib/cjs/index.js"
    },
    "./addon": {
      "types": "./lib/types/client/addon/index.d.ts",
      "default": "./lib/cjs/client/addon/index.js"
    },
    "./preset": "./preset/index.js",
    "./preset/ie11": "./preset/ie11.js"
  },
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "author": "Dmitriy Lazarev <w@kich.dev>",
  "repository": {
    "type": "git",
    "url": "https://github.com/wKich/creevey.git"
  },
  "license": "MIT",
  "scripts": {
    "commit": "git-cz",
    "clean": "rimraf lib",
    "lint": "run-p --continue-on-error lint:*",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --ext .js,.ts,.tsx ./",
    "lint:prettier": "prettier --check *.{md,json,js,ts} ./**/*.{js,ts,tsx}",
    "pretest:e2e": "yarn build && yarn pack -f creevey.tgz",
    "bootstrap:storybook": "cd ../storybook && yarn install && yarn bootstrap --core",
    "test": "run-p test:*",
    "test:unit": "jest --config jest.config.js",
    "start": "run-p start:*",
    "start:client": "webpack-dev-server",
    "start:storybook": "start-storybook --ci -p 6006",
    "start:creevey": "yarn creevey --ui",
    "start:creevey:github": "yarn creevey:github --ui",
    "creevey": "node --require ./scripts/babel-register -- src/cli",
    "creevey:github": "yarn creevey --config .creevey/github.config.ts",
    "creevey:gitlab": "yarn creevey --config .creevey/gitlab.config.ts",
    "creevey:hybrid": "yarn creevey --config .creevey/hybrid.config.ts",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint --fix ./",
    "fix:prettier": "prettier --write .",
    "prebuild": "yarn clean && mkdir lib",
    "build": "run-p --continue-on-error build:*",
    "build:types": "tsc --emitDeclarationOnly -p scripts/tsconfig.prod.json",
    "build:creevey": "run-p \"build:creevey:* --extensions .ts,.tsx --ignore src/client/web\"",
    "build:creevey:cjs": "cross-env BABEL_MODE=cjs babel src --out-dir lib/cjs",
    "build:creevey:esm": "cross-env BABEL_MODE=esm babel src --out-dir lib/esm",
    "build:client": "webpack --mode production",
    "postbuild": "run-p copy:*",
    "copy:index": "cpx scripts/dist/types/* lib/types/",
    "copy:html": "cpx src/client/web/index.html lib/cjs/client/web",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --project-token t3010eknrh",
    "prepublishOnly": "yarn build",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16.0"
  },
  "dependencies": {
    "@babel/code-frame": "^7.18.6",
    "@babel/generator": "^7.18.7",
    "@babel/helpers": "^7.18.6",
    "@babel/parser": "^7.18.8",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.6",
    "@babel/runtime-corejs3": "^7.18.6",
    "@babel/traverse": "^7.18.8",
    "@babel/types": "^7.18.8",
    "@koa/cors": "^3.3.0",
    "@octokit/core": "^4.0.4",
    "@storybook/components": "^6.5.9",
    "@storybook/csf": "0.0.2--canary.87bc651.0",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.9",
    "@types/chai": "^4.3.1",
    "@types/selenium-webdriver": "^4.1.5",
    "airbnb-js-shims": "^2.2.1",
    "babel-plugin-tsconfig-paths": "^1.0.3",
    "chai": "^4.3.6",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "core-js": "^3.23.4",
    "dockerode": "^3.3.2",
    "event-source-polyfill": "1.0.25",
    "find-cache-dir": "^3.3.1",
    "get-tsconfig": "^4.2.0",
    "global-jsdom": "8.5.0",
    "jsdom": "^20.0.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "loader-utils": "^3.2.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.0",
    "loglevel-plugin-prefix": "^0.8.4",
    "micromatch": "^4.0.5",
    "minimist": "^1.2.6",
    "mocha": "~10.0.0",
    "null-loader": "^4.0.1",
    "ora": "^5.4.1",
    "pirates": "^4.0.5",
    "pixelmatch": "^5.3.0",
    "pngjs": "^6.0.0",
    "polished": "^4.2.2",
    "qs": "^6.11.0",
    "regenerator-runtime": "^0.13.7",
    "resolve-from": "^5.0.0",
    "schema-utils": "^4.0.0",
    "selenium-webdriver": "^4.3.1",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "webpack-node-externals": "^3.0.0",
    "whatwg-fetch": "^3.6.2",
    "ws": "^8.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@mdx-js/mdx": "^1.6.22",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.5.9",
    "@storybook/api": "^6.5.9",
    "@storybook/channels": "^6.5.9",
    "@storybook/client-api": "^6.5.9",
    "@storybook/client-logger": "^6.5.9",
    "@storybook/core-client": "^6.5.9",
    "@storybook/core-common": "^6.5.9",
    "@storybook/core-events": "^6.5.9",
    "@storybook/core-server": "^6.5.9",
    "@storybook/preview-web": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@types/babel__code-frame": "^7.0.2",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.17.1",
    "@types/dockerode": "^3.3.9",
    "@types/find-cache-dir": "^3.2.0",
    "@types/koa": "^2.13.1",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/koa-mount": "^4.0.1",
    "@types/koa-static": "^4.0.1",
    "@types/koa__cors": "^3.3.0",
    "@types/loader-utils": "^2.0.2",
    "@types/lodash": "^4.14.182",
    "@types/micromatch": "^4.0.1",
    "@types/minimist": "^1.2.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.43",
    "@types/pixelmatch": "^5.2.2",
    "@types/pngjs": "^6.0.0",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.3",
    "@types/resize-observer-browser": "^0.1.7",
    "@types/shelljs": "^0.8.11",
    "@types/tmp": "^0.2.3",
    "@types/webpack": "^5.28.0",
    "@types/webpack-node-externals": "^2.5.3",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "babel-loader": "^8.2.5",
    "browserstack-local": "^1.5.1",
    "chromatic": "^6.7.0",
    "conventional-changelog-cli": "^2.2.2",
    "cpx": "1.5.0",
    "cross-env": "7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "immer": "^9.0.15",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "require-from-string": "^2.0.2",
    "rimraf": "^3.0.2",
    "tmp": "^0.2.1",
    "typescript": "~4.7.4",
    "use-immer": "^0.7.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  },
  "lint-staged": {
    "**/*.(j|t)s?(x)": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.(md|json)": [
      "prettier --write"
    ]
  },
  "storybook": {
    "displayName": "Creevey"
  },
  "volta": {
    "node": "16.15.0",
    "yarn": "1.22.19"
  }
}
